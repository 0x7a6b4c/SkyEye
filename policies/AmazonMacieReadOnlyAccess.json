{
  "PolicyName": "AmazonMacieReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonMacieReadOnlyAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "macie2:Describe*",
        "macie2:Get*",
        "macie2:List*",
        "macie2:BatchGetCustomDataIdentifiers",
        "macie2:SearchResources"
      ],
      "Resource": "*"
    }
  ]
}