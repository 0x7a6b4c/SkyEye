{
  "PolicyName": "AWSCertificateManagerReadOnly",
  "PolicyArn": "arn:aws:iam::aws:policy/AWSCertificateManagerReadOnly",
  "DefaultVersionId": "v4",
  "Statement": {
    "Effect": "Allow",
    "Action": [
      "acm:DescribeCertificate",
      "acm:ListCertificates",
      "acm:GetCertificate",
      "acm:ListTagsForCertificate",
      "acm:GetAccountConfiguration"
    ],
    "Resource": "*"
  }
}