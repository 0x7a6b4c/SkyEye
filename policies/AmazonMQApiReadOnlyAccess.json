{
  "PolicyName": "AmazonMQApiReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonMQApiReadOnlyAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Action": [
        "mq:Describe*",
        "mq:List*",
        "ec2:DescribeNetworkInterfaces",
        "ec2:DescribeSecurityGroups",
        "ec2:DescribeSubnets",
        "ec2:DescribeVpcs"
      ],
      "Effect": "Allow",
      "Resource": "*"
    }
  ]
}