{
  "PolicyName": "AmazonMSKReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonMSKReadOnlyAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Action": [
        "kafka:Describe*",
        "kafka:List*",
        "kafka:Get*",
        "ec2:DescribeNetworkInterfaces",
        "ec2:DescribeSecurityGroups",
        "ec2:DescribeSubnets",
        "ec2:DescribeVpcs",
        "kms:DescribeKey"
      ],
      "Effect": "Allow",
      "Resource": "*"
    }
  ]
}