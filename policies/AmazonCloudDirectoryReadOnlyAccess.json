{
  "PolicyName": "AmazonCloudDirectoryReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonCloudDirectoryReadOnlyAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "clouddirectory:List*",
        "clouddirectory:Get*",
        "clouddirectory:LookupPolicy",
        "clouddirectory:BatchRead"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}