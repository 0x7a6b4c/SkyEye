  "DefaultVersionId": "v2",
  "Statement": [
    {
      "Sid": "AllowsIotToAccessGreengrassOTAUpdateArtifacts",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::*-greengrass-updates/*"
      ]
    }
  ]
}