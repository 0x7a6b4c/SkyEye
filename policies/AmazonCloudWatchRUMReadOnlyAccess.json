{
  "PolicyName": "AmazonCloudWatchRUMReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonCloudWatchRUMReadOnlyAccess",
  "DefaultVersionId": "v3",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "rum:GetAppMonitor",
        "rum:GetAppMonitorData",
        "rum:ListAppMonitors",
        "rum:ListRumMetricsDestinations",
        "rum:BatchGetRumMetricDefinitions",
        "rum:GetResourcePolicy"
      ],
      "Resource": "*"
    }
  ]
}