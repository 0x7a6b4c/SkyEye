{
  "PolicyName": "AmazonTranscribeFullAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonTranscribeFullAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "transcribe:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::*transcribe*"
      ]
    }
  ]
}