{
  "PolicyName": "AWSIoTWirelessLogging",
  "PolicyArn": "arn:aws:iam::aws:policy/AWSIoTWirelessLogging",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:DescribeLogGroups",
        "logs:DescribeLogStreams",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:log-group:/aws/iotwireless*"
    }
  ]
}