{
  "PolicyName": "AWSBudgetsReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AWSBudgetsReadOnlyAccess",
  "DefaultVersionId": "v2",
  "Statement": [
    {
      "Sid": "AWSBudgetsReadOnlyAccess",
      "Effect": "Allow",
      "Action": [
        "aws-portal:ViewBilling",
        "budgets:ViewBudget",
        "budgets:Describe*",
        "budgets:ListTagsForResource"
      ],
      "Resource": "*"
    }
  ]
}