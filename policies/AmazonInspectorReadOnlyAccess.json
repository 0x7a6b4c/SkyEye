{
  "PolicyName": "AmazonInspectorReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AmazonInspectorReadOnlyAccess",
  "DefaultVersionId": "v4",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "inspector:Describe*",
        "inspector:Get*",
        "inspector:List*",
        "inspector:Preview*",
        "ec2:DescribeInstances",
        "ec2:DescribeTags",
        "sns:ListTopics",
        "events:DescribeRule",
        "events:ListRuleNamesByTarget"
      ],
      "Resource": "*"
    }
  ]
}