{
  "PolicyName": "AWSQuickSightIoTAnalyticsAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/AWSQuickSightIoTAnalyticsAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Action": [
        "iotanalytics:ListDatasets",
        "iotanalytics:DescribeDataset",
        "iotanalytics:GetDatasetContent"
