{
  "PolicyName": "AutoScalingNotificationAccessRole",
  "PolicyArn": "arn:aws:iam::aws:policy/service-role/AutoScalingNotificationAccessRole",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Effect": "Allow",
      "Resource": "*",
      "Action": [
        "sqs:SendMessage",
        "sqs:GetQueueUrl",
        "sns:Publish"
      ]
    }
  ]
}