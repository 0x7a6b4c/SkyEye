{
  "PolicyName": "CloudSearchReadOnlyAccess",
  "PolicyArn": "arn:aws:iam::aws:policy/CloudSearchReadOnlyAccess",
  "DefaultVersionId": "v1",
  "Statement": [
    {
      "Action": [
        "cloudsearch:Describe*",
        "cloudsearch:List*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    }
  ]
}